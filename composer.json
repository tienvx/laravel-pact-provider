{
    "name": "tienvx/pact-provider-package",
    "description": "Pact Provider integration for Laravel",
    "keywords": ["laravel", "pact", "contract-testing", "consumer-driven-contract-testing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tien Vo",
            "email": "tien.xuan.vo@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^9|^10",
        "illuminate/routing": "^9|^10",
        "illuminate/http": "^9|^10",
        "illuminate/support": "^9|^10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Tienvx\\PactProviderPackage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tienvx\\PactProviderPackage\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tienvx\\PactProviderPackage\\ServiceProvider"
            ]
        }
    }
}
